[core]
	pager = less
	editor = vim
[push]
	default = simple
[color]
	ui = auto

[color "diff"]
	commit = 226
	meta = 117
	frag = 117
	old = 217
	new = 118

[color "status"]
	added = 118
	changed = 214
	untracked = 117
	unmerged = 214

[color "decorate"]
	head = 87
	branch = 119
	remotebranch = 117

[color "branch"]
	current = 118
	branch = white
	remote = 117

[log]
	graphColors = 214,119,226,117,212,87,254

[include]
	path = .gitconfig.local

[color "diff-highlight"]
	oldNormal = 217
	oldHighlight = 253 124
	newNormal = 118
	newHighlight = 254 22
