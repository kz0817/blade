[core]
	pager = less
	editor = vim
[push]
	default = simple
[color]
	ui = auto

[color "diff"]
	commit = 226
	meta = 117
	frag = 117
	old = 217
	new = 118

[color "status"]
	added = 118
	changed = 214
	untracked = 117
	unmerged = 214

[color "decorate"]
	head = 87
	branch = 119
	remotebranch = 117

[color "branch"]
	current = 118
	branch = white
	remote = 117

[log]
	graphColors = 217,118,226,75,177,87,254

[include]
	path = .gitconfig.local

[color "diff-highlight"]
	oldNormal = 217
	oldHighlight = 253 88
	newNormal = 118
	newHighlight = 254 22

[alias]
	log1=log --graph --pretty=format:'%C(112)%h %Creset%C(111)%ai %Creset%s %C(30)%an <%ae>  %C(89 253 reverse)%D%Creset'
