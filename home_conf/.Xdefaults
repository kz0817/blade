! How to apply this settings in a running session.
!
!   xrdb -merge .Xdefault
!

!Black Dark/Gray
*color0:  rgb:00/00/00
*color8:  rgb:80/80/80

!Red
*color1:  rgb:cc/20/20
*color9:  rgb:ff/70/70

!Green
*color2:  rgb:49/9a/06
*color10: rgb:8a/e2/34

!Yellow
*color3:  rgb:ed/d4/00
*color11: rgb:fc/e9/4f

!Blue
*color4:  rgb:34/65/a4
*color12: rgb:72/9f/cf

!Megenta
*color5:  rgb:92/65/9a
*color13: rgb:c1/9f/be

!Cyan
*color6:  rgb:07/c7/ca
*color14: rgb:63/e9/e9

!Gray (White)
*color7:  rgb:d3/d7/cf
*color15: rgb:ee/ee/ec

*cursorColor: white

XTerm*foreground: white
XTerm*background: black
XTerm*scrollBar:  false
XTerm*eightBitInput: false
XTerm*eightBitOutput: true
XTerm*font: rk14
XTerm*boldFont: rk14

XTerm*faceName: DejaVu Sans Mono
XTerm*faceNameDoubleSize: MotoyaLCedar
XTerm*faceSize: 10

URxvt.foreground: white
URxvt.background: black
URxvt.scrollBar: false
URxvt.termName: xterm-256color
!URxvt.font: xft:DejaVu Sans Mono:size=10, xft:Noto Sans CJK JP
URxvt.font: xft:DejaVu Sans Mono:size=10, xft:MotoyaLCedar:size=9.5
!URxvt.boldFont: xft:DejaVu Sans Mono:size=10, xft:MotoyaLCedar:size=9.5
URxvt.letterSpace: -1

! line color for underline
URxvt.colorUL:        rgb:ee/ee/ec
URxvt.underlineColor: rgb:a0/a0/a0
